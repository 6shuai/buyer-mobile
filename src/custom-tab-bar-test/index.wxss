.tab-bar {
  width: 100%;
	height: 164rpx;
	background: rgba(255, 255, 255, 0.8);
	position: fixed;
	bottom: 0;
	left: 0;
	display: flex;
  z-index: 99999;
}

.tab-bar-item {
  flex: 1;
  text-align: center;
  margin-top: 6rpx;
}

.tab-bar-item cover-image {
  width: 96rpx;
  height: 96rpx;
  display: inline-block;
}

.tab-bar-item cover-image.tab_bar_home{
  width: 128rpx;
  height: 128rpx;
  margin-top: -40rpx;
}