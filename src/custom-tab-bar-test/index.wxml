<!--miniprogram/custom-tab-bar/index.wxml-->
<view class="tab-bar">
  <view 
    wx:for="{{list}}" 
    wx:key="index" 
    class="tab-bar-item" 
    data-path="{{item.pagePath}}" 
    data-index="{{index}}" 
    bindtap="switchTab"
  >
    <cover-image 
      class="{{ index ? '' : 'tab_bar_home' }}"
      src="{{selected === index ? item.selectedIconPath : item.iconPath}}"
    ></cover-image>
    {{selected}}
  </view>
</view>
